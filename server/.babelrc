{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "data": "./src/data",
        "generated": "./src/generated",
        "models": "./src/models",
        "types": "./src/types"
      },
      "extensions": [".js", ".ts"]
    }]
  ],
  "presets": ["@babel/preset-env", "@babel/preset-typescript"]
}
