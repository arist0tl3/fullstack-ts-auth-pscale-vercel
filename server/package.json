{
  "name": "typescript-express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/babel ./src -d dist --extensions .ts,.js --ignore \"node_modules/**/*.js\"",
    "deploy-vercel": "npm run build && vercel",
    "serve": "pscale connect my-app main --execute 'NODE_ENV=production node dist/index.js'",
    "start": "pscale connect my-app feature --execute 'nodemon --exec babel-node --extensions .ts,.js ./src/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "apollo-server-express": "^2.24.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
